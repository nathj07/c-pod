# Allow access via remote proxy tunnel
#
Host fill-in-the-remote-host-you-want-to-access-via-proxy.local
ProxyCommand /usr/bin/nc -X 5 -x <address-of-cpod>:1080 %h %p

# General defaults

Host *
StrictHostKeyChecking no
CheckHostIP	yes
ForwardAgent yes
GSSAPIAuthentication  no
ServerAliveInterval 30
